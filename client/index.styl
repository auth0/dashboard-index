// infowindow

.info-wrap{
  background-color: #333;
  float: left;
}

#dashboard .chart {
  background: #f9f9f9;
  padding: 20px 0;
  border-radius: 3px;
}

#dashboard .chart h5 {
  font-size: 13px;
  font-weight: 400;
  color: rgba(0,0,0,0.7);
  margin: 0;
  padding: 0;
  border: none;
  text-align: center;
  text-transform: none;
}

#dashboard .row-stats span {
  font-size: 11px;
  letter-spacing: .79;
}

#dashboard .row-stats strong {
  font-size: 40px;
}

#dashboard .row-stats small {
  opacity: .7;
}

#dashboard .stat-box {
  float: left;
  width: 20%;
}

#dashboard .stat-box.stat-box-suspicious, #dashboard .stat-box.stat-box-suspicious strong {
  color: #EB5424;
}

#dashboard .row-stats {
  height: auto;
  border: none;
  margin: 0;
  border-bottom: 1px solid #eee;
  padding: 40px 0;
  margin-bottom: 40px;
  box-sizing: content-box;
}

#dashboard .col-widgets {
  padding-right: 15px;
  padding-left: 15px;
}

#dashboard .col-widgets h5 {
  margin: 0;
  padding: 0 0 10px;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  font-size: 12px;
  font-weight: 400;
  letter-spacing: .86;
  color: #000;
}

#dashboard .col-widgets .widget-content p {
  margin: 15px 0;
  font-size: 13px;
  color: rgba(0,0,0,.89);
  text-align: center;
}

#dashboard .col-widgets .widget-content p span {
  display: block;
}

#dashboard .col-widgets .widget-content .try-now {
  color: #33B6E3;
  margin-top: 10px;
  display: inline-block;
}

#dashboard .user-list {
  margin-top: 15px;
}

#dashboard .user-list li {
  padding: 0;
}

#dashboard .user-list li a {
  display: block;
}

#dashboard .user-list li figure
  position relative
  float left
  width 50px
  height 50px

#dashboard .user-list li span.social-login
  width 20px
  height 20px
  position absolute
  bottom 0
  right 0

  &.twitter
    background-image url('data:image/svg+xml;base64,');

  &.facebook
    background-image url('data:image/svg+xml;base64,');

  &.instagram
    background-image url('data:image/svg+xml;base64,');

  &.github
    background-image url('data:image/svg+xml;base64,');

#dashboard .user-list .avatar {
  width: 50px;
  top: 0;
}

#dashboard .user-list .description-content
  padding-left 65px

#dashboard .user-list time {
  font-size: 12px;
  color: #5D676F;
}

#dashboard .user-list li span.username {
  margin: 0;
  font-weight: 400;
}

#dashboard .user-list li span {
  color: #5D676F;
  margin-top: 12px;
}